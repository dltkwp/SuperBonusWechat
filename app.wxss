/*公用型列表*/

.list-block {
  width: 100%;
  position: relative;
  border-top: 1rpx solid #ecedef;
  border-bottom: 1rpx solid #ecedef;
}

.list-block ul {
  list-style: none;
  padding: 0;
  margin: 0;
  position: relative;
  box-sizing: border-box;
}

.list-block ul li {
  background-color: #fff;
  box-sizing: border-box;
  position: relative;
}

.list-block .item-media {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  flex-shrink: 0;
  -webkit-box-lines: single;
  -moz-box-lines: single;
  -webkit-flex-wrap: nowrap;
  flex-wrap: nowrap;
  box-sizing: border-box;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding-top: 16rpx;
  padding-bottom: 16rpx;
}

.list-block.media-list .item-media {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.list-block.media-list .item-media img {
  display: block;
}

.list-block .item-inner {
  padding-right: 30rpx;
  position: relative;
  width: 100%;
  padding-top: 16rpx;
  padding-bottom: 16rpx;
  min-height: 88rpx;
  overflow: hidden;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  border-bottom: 1rpx solid #ecedef;
}

.list-block li:last-child .item-inner {
  border-bottom: none;
}

.list-block.media-list .item-inner {
  display: block;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  -webkit-align-self: stretch;
  align-self: stretch;
}

.list-block.media-list .item-media {
  margin-right: 20rpx;
}

.list-block .item-title-row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}

.list-block .item-subtitle {
  position: relative;
  overflow: hidden;
  max-width: 100%;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  height: 76rpx;
  -webkit-box-orient: vertical;
  display: -webkit-box;
}

.list-block .item-text {
  position: relative;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  display: -webkit-box;
}

.list-block .item-title {
  flex-shrink: 1;
  position: relative;
  text-overflow: ellipsis;
  max-width: 100%;
}

.list-block .item-title.label {
  width: 35%;
  flex-shrink: 0;
  margin: 8rpx 0;
}

.list-block .item-input {
  width: 100%;
  -webkit-box-flex: 1;
  flex-shrink: 1;
}

.list-block .item-input-lg input {
  height: 80rpx;
  line-height: 80rpx;
}

.list-block .item-after {
  white-space: nowrap;
  flex-shrink: 0;
  margin-left: 5rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
